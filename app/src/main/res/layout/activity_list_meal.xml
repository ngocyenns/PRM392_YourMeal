<?xml version="1.0" encoding="utf-8"?>

<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context=".views.ListMealActivity">


    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:gravity="center"
        android:orientation="horizontal"
        android:padding="10dp">

        <ImageView
            android:layout_width="40dp"
            android:layout_height="40dp"
            android:layout_alignParentStart="true"
            android:contentDescription="TODO"
            android:src="@drawable/arrow_back_pink" />

        <TextView
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:gravity="center"
            android:text="List Meal"
            android:textColor="@color/black"
            android:textSize="32sp" />
    </LinearLayout>

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="50dp"
        android:padding="10dp">

        <SearchView
            android:id="@+id/simpleSearchView"
            android:layout_width="220dp"
            android:layout_height="wrap_content"
            android:background="#FAFAFE"
            android:iconifiedByDefault="false"
            android:queryHint="Search" />

        <ImageView
            android:layout_width="40dp"
            android:layout_height="40dp"
            android:layout_alignParentEnd="true"
            android:contentDescription="TODO"
            android:src="@drawable/filter_pink" />
    </RelativeLayout>

    <LinearLayout
        android:id="@+id/auto_layout1"
        android:layout_width="312dp"
        android:layout_height="88dp"
        android:layout_gravity="center"
        android:layout_marginTop="32dp"
        android:background="@drawable/auto_layout"
        android:orientation="horizontal">

        <ImageView
            android:layout_width="88dp"
            android:layout_height="88dp"
            android:src="@drawable/meal1" />

        <LinearLayout
            android:layout_width="109dp"
            android:layout_height="match_parent"
            android:orientation="vertical"
            android:paddingLeft="8dp">

            <TextView
                android:id="@+id/original_sa1"

                android:layout_width="80dp"
                android:layout_height="30dp"
                android:layout_marginTop="16dp"
                android:lineSpacingExtra="4sp"
                android:text="Mi Y"
                android:textColor="#09101D"
                android:textSize="18dp"
                android:translationY="-2.19sp" />

            <TextView

                android:id="@+id/lovy_food1"
                android:layout_width="60dp"
                android:layout_height="21dp"
                android:layout_gravity=""
                android:gravity="top"
                android:lineSpacingExtra="3sp"
                android:text="Lovy Food"
                android:textColor="#858C94"
                android:textSize="18dp"
                android:translationY="-1.7sp" />
        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:gravity="right">

            <TextView
                android:layout_marginRight="8pt"
                android:id="@+id/gia1"
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:gravity="center"
                android:lineSpacingExtra="3sp"
                android:text="$8"
                android:textColor="#F43F5E"
                android:textSize="29sp"
                android:translationY="-1.7sp" />
        </LinearLayout>


    </LinearLayout>
